<?xml version="1.0"?>
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2">
 <!--Cyr K hook(Óƒ)-->
 <glyph ps-name="uni04C3" init-graphics="yes">
  
  <!--
      
