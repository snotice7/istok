<?xml version="1.0"?>
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2">
 <!--Cyr Ch leftdesc(Ó‹)-->
 <glyph ps-name="uni04CB" init-graphics="yes">
  
  <!--
      
