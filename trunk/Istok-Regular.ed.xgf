 <control-value name="sc-x-height" value="1120"/>
 <control-value name="sc-o-height" value="1137"/>
 <control-value name="sc-a-hstem-height" value="296"/>
 <control-value name="sc-h-intersect-height" value="517"/>
 <control-value name="sc-b-low-space" value="816"/>
 <control-value name="sc-h-space" value="856"/>
