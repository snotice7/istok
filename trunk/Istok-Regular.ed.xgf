 <alias name="period-height" target="period-width"/>
 <xi:include href="upr_functions.xgf" xpointer="xmlns(x=http://xgridfit.sourceforge.net/Xgridfit2)xpointer(//x:function)"/>
 <xi:include href="upr_functions.xgf" xpointer="xmlns(x=http://xgridfit.sourceforge.net/Xgridfit2)xpointer(//x:macro)"/>
 
 <pre-program>
  <set-dropout-type value="5"/>
  <if test="pixels-per-em &gt; 11">
   <!-- corrections for antialiasing -->
   <if test="is-glyph-grayscale or (is-cleartype-enabled and not(is-BGR-order))">
    <set-equal target="horizontal-smoothing" source="1"/>
    <set-equal target="any-smoothing" source="1"/>
    <if test="not(is-glyph-grayscale)">
     <set-equal target="only-horizontal-smoothing" source="1"/>
    </if>
   </if>
   <if test="is-glyph-grayscale or (is-cleartype-enabled and is-BGR-order)">
    <set-equal target="vertical-smoothing" source="1"/>
    <if test="not(is-glyph-grayscale)">
     <set-equal target="only-vertical-smoothing" source="1"/>
     <set-equal target="any-smoothing" source="1"/>
    </if>
   </if>
  </if>
  
  <call-macro name="regularize-control-value-width">
   <with-param name="cv-width" value="cap-o-space"/>
   <with-param name="margin" value="cap-o-lbearing"/>
  </call-macro>
  <call-macro name="regularize-control-value-width">
   <with-param name="cv-width" value="small-o-space"/>
   <with-param name="margin" value="small-o-lbearing"/>
  </call-macro>
  <call-macro name="regularize-control-value-width">
   <with-param name="cv-width" value="cap-h-space"/>
   <with-param name="margin" value="cap-h-lbearing"/>
  </call-macro>
  <call-macro name="regularize-control-value-width">
   <with-param name="cv-width" value="small-cyrn-space"/>
   <with-param name="margin" value="small-cyrn-lbearing"/>
  </call-macro>
  <call-macro name="regularize-control-value-width">
   <with-param name="cv-width" value="cap-cyri-space"/>
   <with-param name="margin" value="cap-h-lbearing"/>
  </call-macro>
  <call-macro name="regularize-control-value-width">
   <with-param name="cv-width" value="small-cyri-space"/>
   <with-param name="margin" value="small-cyrn-lbearing"/>
  </call-macro>
  <call-macro name="regularize-control-value-width-two-margin">
   <with-param name="cv-width" value="small-p-space"/>
   <with-param name="left-margin" value="small-cyrn-lbearing"/>
   <with-param name="right-margin" value="small-o-lbearing"/>
  </call-macro>
  <call-macro name="regularize-control-value-width-two-varmargin">
   <with-param name="cv-width" value="digit-six-hspace"/>
   <with-param name="left-margin" value="(control-value(emspace) * 9.2p) / 100p"/>
   <with-param name="right-margin" value="(control-value(emspace) * 9.0p) / 100p"/>
  </call-macro>
  
  <round value="cap-x-height"/>
  <round value="cap-tse-desc"/>
  <round value="cap-a-hstem-height"/>
  <round value="cap-h-intersect-height"/>
  <round value="cap-o-baseline"/>
  <round value="small-asc-height"/>
  <round value="cap-h-lbearing" color="white"/>
  <round value="cap-o-lbearing" color="white"/>
  <!--   <round value="cap-o-space" color="black"/> -->
  <round value="cap-cyrz-lbearing" color="white"/>
  <round value="cap-b-low-space" color="black"/>
  <round value="cap-p-space" color="black"/>
  <round value="cap-cyrl-space" color="black"/>
  <round value="cap-cyrch-space" color="black"/>
  <round value="cap-p-curve-stem" color="black"/>
  <round value="cap-tse-desc-horiz-shift"/>
  <round value="cap-sh-inner-space"/>
  <round value="small-x-height"/>
  <round value="small-o-baseline"/>
  <round value="small-curve-desc"/>
  <round value="small-tse-desc"/>
  <round value="small-cyrn-intersect-height"/>
  <round value="small-cyrn-lbearing" color="white"/>
  <round value="small-o-lbearing" color="white"/>
  <round value="small-cyrz-lbearing" color="white"/>
  <round value="small-cyrv-low-space" color="black"/>
  <!--   <round value="small-o-space" color="black"/> -->
  <round value="small-cyrl-space" color="black"/>
  <round value="small-cyrch-space" color="black"/>
  <round value="small-curve-vstem" color="black"/>
  <round value="small-tse-desc-horiz-shift"/>
  <round value="small-m-inner-space"/>
  <round value="small-flig-space" color="black"/>
  <round value="sc-x-height"/>
  <round value="sc-a-hstem-height"/>
  <round value="sc-h-intersect-height"/>
  <round value="sc-h-space" color="black"/>
  <round value="sc-o-space" color="black"/>
  <round value="sc-p-space" color="black"/>
  <round value="sc-b-low-space" color="black"/>
  <round value="sc-cyri-space" color="black"/>
  <round value="sc-cyrl-space" color="black"/>
  <round value="sc-cyrch-space" color="black"/>
  <round value="sc-sh-inner-space"/>
  <round value="digit-height"/>
  <round value="digit-over-height"/>
  <round value="digit-six-inner-height" color="black"/>
  <round value="period-width" color="black"/>
  <round value="minus-thickness" color="black"/>
  
  <set-equal target="cap-o-height" source="control-value(index(cap-x-height)) - control-value(index(cap-o-baseline))"/>
  <set-equal target="small-o-height" source="control-value(index(small-x-height)) - control-value(index(small-o-baseline))"/>
  <set-equal target="small-asc-over-height" source="control-value(index(small-asc-height)) - control-value(index(small-o-baseline))"/>
  <set-equal target="sc-o-height" source="control-value(index(sc-x-height)) - control-value(index(small-o-baseline))"/>
  <set-equal target="digit-over-height" source="control-value(index(digit-height)) - control-value(index(cap-o-baseline))"/>
  
  <if test="is-glyph-grayscale">
   <set-control-value-cut-in value="1.06p"/>
   <if test="pixels-per-em &lt; 21 and pixels-per-em &gt; 11">
    <round value="cap-curve-hstem" color="black"/>
    <round value="cap-horiz-stem" color="black"/>
    <round value="cap-vert-thinstem" color="black"/>
    <round value="cap-vert-stem" color="black"/>
    <round value="cap-o-curve-stem" color="black"/>
    <round value="small-curve-hstem" color="black"/>
    <round value="small-horiz-stem" color="black"/>
    <round value="small-vert-thinstem" color="black"/>
    <round value="small-vert-stem" color="black"/>
    <round value="small-o-curve-stem" color="black"/>
    <round value="minus-thickness" color="black"/>
    <round value="digit-mid-horiz-stem" color="black"/>
    <round value="digit-thin-horiz-stem" color="black"/>
    <round value="digit-thicker-curve-stem" color="black"/>
    <round value="digit-thinner-curve-stem" color="black"/>
    <control-value-delta>
     <delta-set cv="cap-o-curve-stem" size="6" distance="-8"/>
     <delta-set cv="cap-vert-stem" size="7" distance="-8"/>
     <delta-set cv="cap-o-curve-stem" size="7" distance="-8"/>
     <delta-set cv="small-o-curve-stem" size="8" distance="-8"/>
     <delta-set cv="cap-vert-stem" size="8" distance="-8"/>
     <delta-set cv="cap-o-curve-stem" size="8" distance="-8"/>
     <delta-set cv="cap-horiz-stem" size="10" distance="-8"/>
     <delta-set cv="cap-horiz-stem" size="11" distance="-8"/>
     <delta-set cv="cap-curve-hstem" size="11" distance="-8"/>
    </control-value-delta>
   </if>
  </if>
  
  <control-value-delta>
   <delta-set cv="cap-p-curve-stem" size="7" distance="-8"/>
   <delta-set cv="cap-p-curve-stem" size="8" distance="-8"/>
   <delta-set cv="small-cyrl-space" size="8" distance="-8"/>
   <delta-set cv="digit-mid-horiz-stem" size="10" distance="-8"/>
   <delta-set cv="digit-mid-horiz-stem" size="11" distance="-8"/>
   <delta-set cv="digit-mid-horiz-stem" size="12" distance="-4"/>
   <!--    <delta-set cv="digit-mid-horiz-stem" size="13" distance="-8"/> -->
   <!--    <delta-set cv="digit-mid-horiz-stem" size="14" distance="-8"/> -->
   <!--    <delta-set cv="digit-mid-horiz-stem" size="15" distance="-8"/> -->
   <delta-set cv="digit-thicker-curve-stem" size="7" distance="-8"/>
   <delta-set cv="digit-thicker-curve-stem" size="8" distance="-8"/>
   <delta-set cv="digit-thicker-curve-stem" size="18" distance="-1"/>
   <delta-set cv="digit-thinner-curve-stem" size="8" distance="-8"/>
   <delta-set cv="digit-thinner-curve-stem" size="18" distance="-1"/>
   <delta-set cv="digit-thin-horiz-stem" size="12" distance="-3"/>
  </control-value-delta>
  
  <if test="control-value(small-o-lbearing) = 0">
   <set-equal target="small-o-lbearing" source="1.0p"/>
  </if>
  <if test="control-value(cap-h-space) &lt; control-value(cap-cyrch-space)">
   <set-equal target="cap-cyrch-space" source="control-value(cap-h-space)"/>
  </if>
  <if test="control-value(small-cyrn-space) &lt; control-value(small-cyrch-space)">
   <set-equal target="small-cyrch-space" source="control-value(small-cyrn-space)"/>
  </if>
  <if test="control-value(sc-h-space) &lt; control-value(sc-cyrch-space)">
   <set-equal target="sc-cyrch-space" source="control-value(sc-h-space)"/>
  </if>
  <set-equal target="temp" source="round-black(control-value(index(cap-vert-stem))) * 0.89p"/>
  <if test="control-value(index(cap-diag)) &gt; temp">
   <set-equal target="cap-diag" source="temp"/>
  </if>
  <set-equal target="temp" source="round-black(control-value(index(cap-vert-thinstem))) * 0.7p"/>
  <if test="control-value(index(cap-thindiag)) &gt; temp">
   <set-equal target="cap-thindiag" source="temp"/>
  </if>
  <set-equal target="temp" source="round-black(control-value(index(small-vert-stem))) * 0.89p"/>
  <if test="control-value(index(small-diag)) &gt; temp">
   <set-equal target="small-diag" source="temp"/>
  </if>
  <set-equal target="temp" source="round-black(control-value(index(small-vert-thinstem))) * 0.7p"/>
  <if test="control-value(index(small-thindiag)) &gt; temp">
   <set-equal target="small-thindiag" source="temp"/>
  </if>
  <set-equal target="temp" source="round-black(control-value(index(cap-vert-stem))) * 0.7p"/>
  <if test="control-value(index(cap-end-width)) &gt; temp">
   <set-equal target="cap-end-width" source="temp"/>
  </if>
  <set-equal target="temp" source="round-black(control-value(index(small-vert-stem))) * 0.7p"/>
  <if test="control-value(index(small-end-width)) &gt; temp">
   <set-equal target="small-end-width" source="temp"/>
  </if>
  <set-equal target="temp" source="round-black(control-value(index(minus-thickness))) * 0.7p"/>
  <if test="control-value(index(sign-thindiag)) &gt; temp">
   <set-equal target="sign-thindiag" source="temp"/>
  </if>
  <call-macro name="regularize-midstem">
   <with-param name="midstem-height" value="cap-h-intersect-height"/>
   <with-param name="stem-width" value="cap-horiz-stem"/>
   <with-param name="full-height" value="cap-x-height"/>
  </call-macro>
  <call-macro name="regularize-midstem">
   <with-param name="midstem-height" value="sc-h-intersect-height"/>
   <with-param name="stem-width" value="small-horiz-stem"/>
   <with-param name="full-height" value="sc-x-height"/>
  </call-macro>
  <call-macro name="regularize-midstem">
   <with-param name="midstem-height" value="small-cyrn-intersect-height"/>
   <with-param name="stem-width" value="small-horiz-stem"/>
   <with-param name="full-height" value="small-x-height"/>
  </call-macro>
  <call-macro name="regularize-control-value-double-width">
   <with-param name="cv-width" value="cap-sh-inner-space"/>
   <with-param name="stem-width" value="cap-vert-stem"/>
   <with-param name="margin" value="cap-h-lbearing"/>
   <with-param name="full-width" value="control-value(emspace) * 1.99p"/>
  </call-macro>
  <call-macro name="regularize-control-value-double-width">
   <with-param name="cv-width" value="small-m-inner-space"/>
   <with-param name="stem-width" value="small-vert-stem"/>
   <with-param name="margin" value="small-cyrn-lbearing"/>
   <with-param name="full-width" value="control-value(emspace) * 1.63p"/>
  </call-macro>
  <call-macro name="regularize-control-value-double-width">
   <with-param name="cv-width" value="sc-sh-inner-space"/>
   <with-param name="stem-width" value="small-vert-stem"/>
   <with-param name="margin" value="small-cyrn-lbearing"/>
   <with-param name="full-width" value="control-value(emspace) * 1.65p"/>
  </call-macro>
 </pre-program>
 
