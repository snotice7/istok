  <param name="diag" value="sign-thindiag"/>
  
  <constant name="left-bottom" value="first-point"/>
  <constant name="left-top" value="left-bottom + vstem-vcount"/>
  <constant name="right-top" value="left-top + vstem-hcount"/>
  <constant name="right-bottom" value="right-top + vstem-vcount"/>
  <constant name="last" value="right-bottom + last-count"/>
  
  <line name="left-side">
   <point num="left-bottom"/>
   <point num="left-top"/>
  </line>
  <line name="right-side">
   <point num="right-bottom"/>
   <point num="right-top"/>
  </line>
  
  <set-vectors axis="y"/>
  <move>
   <point num="left-bottom"/>
  </move>
  <move>
   <point num="left-top"/>
  </move>
  
  <set-vectors axis="x"/>
  <move round="to-double-grid" min-distance="no" color="white">
   <reference>
    <point num="left-bearing"/>
   </reference>
   <point num="left-bottom"/>
   <move>
    <point num="left-top"/>
   </move>
  </move>
  
  <with-round-state round="no">
   <with-minimum-distance value="diag-min-thick">
    <diagonal-stem distance="sign-thindiag" save-vectors="yes" color="black">
     <line ref="left-side"/>
     <line ref="right-side"/>
    </diagonal-stem>
   </with-minimum-distance>
  </with-round-state>
  <!--<compile-if test="slanted">
   <set-projection-vector x-component="x-skew1" y-component="y-skew1"/>
  </compile-if>
      <set-equal target="width" source="round(right-top - - right-bearing) - rbearing"/>
  <compile-if test="slanted">
   <set-projection-vector axis="x"/>
   <round value="width"/>
  </compile-if>
  <if test="width &lt; 0.0p">
   <shift-absolute pixel-distance="width">
    <point num="left-top"/>
    <point num="right-top"/>
   </shift-absolute>
  </if>-->
  <with-vectors to-line="parallel">
   <line ref="left-side"/>
   <align>
    <reference>
     <point num="left-top"/>
    </reference>
    <point num="right-top"/>
   </align>
   <align>
    <reference>
     <point num="left-bottom"/>
    </reference>
    <point num="right-bottom"/>
   </align>
  </with-vectors>
  <move round="no" min-distance="no">
   <reference>
    <point num="right-top"/>
   </reference>
   <point num="right-bearing"/>
  </move>
  <mdap round="yes">
   <point num="right-bearing"/>
  </mdap>
  
  <interpolate-untouched-points/>
 </glyph>
 
</xgridfit>
