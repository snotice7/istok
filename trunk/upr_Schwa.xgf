<?xml version="1.0"?>
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2">
 <!--uni0259 (É™)-->
 <glyph ps-name="uni04D8" init-graphics="yes">
  
  <!--
      
