<?xml version="1.0"?>
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2">
 <!--Cyr G hook (Ò”)-->
 <glyph ps-name="uni0494" init-graphics="yes">
  <!--
      
