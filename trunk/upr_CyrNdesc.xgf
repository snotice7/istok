<?xml version="1.0"?>
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2">
 <!--Cyr N desc(Ò¢)-->
 <glyph ps-name="uni04A2" init-graphics="yes">
  
  <!--
      
