<?xml version="1.0"?>
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2">
 <!--Cyr hard sign (ÑŠ)-->
 <glyph ps-name="afii10092" init-graphics="yes">
  
  <!--
      
