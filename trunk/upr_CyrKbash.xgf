<?xml version="1.0"?>
<xgridfit xmlns="http://xgridfit.sourceforge.net/Xgridfit2">
 <!--Bashkir K (ма)-->
 <glyph ps-name="uni04A0" init-graphics="yes">
  
  <!--
      
